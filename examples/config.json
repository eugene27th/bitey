{
    "port": 30000,

    "headers": [
        "cookie",
        "session",
        "user-agent"
    ],

    "cors": {
        "origin": [
            "http://127.0.0.1:3000",
            "https://domain.com"
        ],
        "credentials": true
    },

    "guard": {
        "http": [60, 10],
        "ws": [10, [60, 10]]
    },

    "logger": {
        "folder": ".logs",
        "interval": 10
    },

    "mysql": {
        "host": "127.0.0.1",
        "user": "username",
        "database": "databasename",
        "password": "password",
        "connectionLimit": 10,
        "metaAsArray": true,
        "insertIdAsNumber": true,
        "timezone": "+02:00",

        "waitForConnections": true,
        "queueLimit": 0,
        "multipleStatements": true
    },
    
    "redis": {
        "host": "127.0.0.1",
        "port": 6379,
        "password": "password"
    },

    "cloudflare": {
        "turnstile": {
            "secret_key": "secret_key",
            "site_key": "site_key"
        }
    }
}