{
    "port": 30000,

    "headers": [
        "origin",
        "content-type",
        "content-length",
        "host",
        "user-agent",
        "accept",
        "accept-encoding",
        "connection",
        "cache-control",
        "cookie",
        "session",
        "cf-connecting-ip",
        "cf-ipcountry",
        "challenge"
    ],

    "cors": {
        "origin": [
            "http://127.0.0.1:3000",
            "https://domain.com"
        ],
        "credentials": true
    },

    "guard": {
        "http": {
            "*": [60, 10],
            "get": [60, 10],
            "post": [30, 10],
            "patch": [10, 10],
            "del": [10, 10]
        }
    },

    "logger": {
        "interval": 10
    },

    "session": {
        "name": "bitey-example",
        "secret": "something-secret",
        "cookie": {
            "path": "/",
            "age": 604800,
            "domain": ".domain.name",
            "samesite": "None",
            "secure": true
        }
    },

    "mysql": {
        "host": "127.0.0.1",
        "user": "username",
        "database": "databasename",
        "password": "password",
        "waitForConnections": true,
        "connectionLimit": 1,
        "queueLimit": 0,
        "multipleStatements": true,
        "timezone": "+02:00"
    },
    
    "redis": {
        "host": "127.0.0.1",
        "port": 6379,
        "password": "password"
    },

    "cloudflare": {
        "turnstile": {
            "secret_key": "secret_key",
            "site_key": "site_key"
        }
    }
}